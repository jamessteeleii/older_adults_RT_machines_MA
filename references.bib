@article{mouraFunctionalCapacityImproves2018,
  title = {Functional Capacity Improves In-Line with Neuromuscular Performance after 12~Weeks of Non-Linear Periodization Strength Training in the Elderly},
  author = {de Moura, Bruno Monteiro and Sakugawa, Raphael Luiz and Orssatto, Lucas Bet da Rosa and {de Lima}, Luis Antonio Pereira and Pinto, Ronei Silveira and Walker, Simon and Diefenthaeler, Fernando},
  year = {2018},
  month = aug,
  journal = {Aging Clinical and Experimental Research},
  volume = {30},
  number = {8},
  pages = {959--968},
  issn = {1720-8319},
  doi = {10.1007/s40520-017-0873-x},
  abstract = {BACKGROUND: While it is accepted that resistance training can improve functional capacity in older individuals, the neuromuscular source of this improvement has yet to be identified. AIM: This study investigated the link between improved neuromuscular performance and functional capacity after a 12-week resistance training period in untrained healthy older individuals. METHODS: Fifteen older men and women (60-71~years) adhered to a 4-week control period, followed by 12~weeks of non-linear resistance training for the lower limbs. Maximum dynamic leg press strength (1-RM), maximum isometric knee extension torque and rate of torque development (RTD) were evaluated at -~4, 0, 4, 8, and 12~weeks, and muscle activity was assessed at 0, 4, 8, and 12~weeks. Functional capacity tests (chair rise, stair ascent and descent, and timed up and go) were performed at -~4, 0, and 12~weeks. RESULTS: No changes occurred during the control period, but the group increased their 1-RM strength (from 142\,{\textpm}\,53 to 198\,{\textpm}\,43~kg, p\,=\,0.001), which was accompanied by an increase in vastus lateralis activation (p\,=\,0.008) during the intervention. Increase was observed at all RTD time intervals at week 8 (p\,{$<$}\,0.05). Significant improvements in all the functional capacity tests were observed at week 12 (p\,{$<$}\,0.05). DISCUSSION: Despite the expected increase in strength, RTD, muscle activity, and functional capacity, there was no significant relationship between the changes in neuromuscular performance and functional capacity. While resistance training elicits various positive improvements in healthy older individuals, actual strength gain did not influence the gain in functional capacity. CONCLUSION: The present study highlights the exact cause that improved the functional capabilities during resistance training are currently unknown.},
  langid = {english},
  pmid = {29214519},
  keywords = {Aged,Aging,Chair rise,Female,Humans,Male,Middle Aged,Muscle Skeletal,Muscle Strength,Quadriceps Muscle,Rate of torque development,Resistance Training,Strength training,Timed up and go,Torque}
}

@article{munoz-bermejoTestRetestReliabilityFive2021,
  title = {Test-{{Retest Reliability}} of {{Five Times Sit}} to {{Stand Test}} ({{FTSST}}) in {{Adults}}: {{A Systematic Review}} and {{Meta-Analysis}}},
