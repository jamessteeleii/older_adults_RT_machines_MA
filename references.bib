@article{filhoEffectsDifferentTypes2022,
  title = {Effects of {{Different Types}} of {{Resistance Training}} and {{Detraining}} on {{Functional Capacity}}, {{Muscle Strength}}, and {{Power}} in {{Older Women}}: {{A Randomized Controlled Study}}},
  shorttitle = {Effects of {{Different Types}} of {{Resistance Training}} and {{Detraining}} on {{Functional Capacity}}, {{Muscle Strength}}, and {{Power}} in {{Older Women}}},
  author = {Filho, Mauro Mazini and Venturini, Gabriela R. D. O. and Moreira, Osvaldo C. and Leit{\~a}o, Luis and Mira, Pedro A. C. and {de Castro}, Juliana B. P. and Aidar, Felipe J. and Novaes, Jefferson da Silva and Vianna, Jeferson M. and Caputo Ferreira, Maria E.},
  year = {2022},
